              "message": {
                "name": "Message",
                "desc": "Enter a message."
              },
              "duration": {
                "name": "Duration",
                "desc": "Select duration",
                "items": {
                  "short (2 seconds)": "short (2 seconds)",
                  "long (5 seconds)": "long (5 seconds)"
                }
              },
              "position": {
                "name": "Position",
                "desc": "Select position",
                "items": {
                  "top": "top",
                  "center": "center",
                  "bottom": "bottom"
                }
              }
            }
          }
        },
        "expressions": {}
      }
    }
  }
